{
    "name": "akrista/eureka",
    "type": "project",
    "description": "Eureka Vepagos",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2|^8.0",
        "barryvdh/laravel-dompdf": "^2.0.0",
        "beyondcode/laravel-websockets": "1.13.2",
        "caffeinated/modules": "^6.2",
        "doctrine/dbal": "^4.2",
        "fideloper/proxy": "^4.0",
        "fruitcake/laravel-cors": "^3.0",
        "igorescobar/jquery-mask-plugin": "^1.14",
        "laravel/framework": "^8.0",
        "laravel/horizon": "^5.10",
        "laravel/legacy-factories": "^1.1",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "laravelcollective/html": "^6.2",
        "livewire/livewire": "^2.3",
        "maatwebsite/excel": "^3.0.1",
        "phpoffice/phpword": "^1.0.0",
        "protoqol/prequel": "^1.23",
        "pusher/pusher-php-server": "^7.2.2",
        "spatie/laravel-permission": "^5.7",
        "yajra/laravel-datatables-oracle": "^9.15",
        "yoeunes/toastr": "^1.2"
    },
    "require-dev": {
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.4",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/pint": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Spatie\\": "app/overrides/spatie/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
